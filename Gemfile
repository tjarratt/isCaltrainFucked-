source 'https://rubygems.org'

gem 'rails', '~> 4.2.5'
gem 'pg'
gem 'unicorn'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
end

gem 'jquery-rails'
gem 'httparty'
gem 'ri_cal'
gem 'sqlite3'

group :development, :test do
  gem 'rspec-rails'
  gem 'pry'
  gem 'pry-nav'
  gem 'powder'
  gem 'dotenv-rails'
end

gem 'twitter'
gem 'sentimental'
gem 'pony'
